{
  "python.defaultInterpreterPath": "${workspaceFolder}/server/.venv/Scripts/python.exe",
  "python.analysis.extraPaths": ["${workspaceFolder}/server/src"],
  "python.autoComplete.extraPaths": ["${workspaceFolder}/server/src"],
  "python.envFile": "${workspaceFolder}/server/.env",
  "python.testing.pytestArgs": ["server"],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "files.exclude": {
    "**/__pycache__": true
    // "**/__init__.py": true
  },
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true
  },
  "python.analysis.autoImportCompletions": true
}
